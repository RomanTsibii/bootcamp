<div class="index">
  <h1>All users</h1>
  <% @users.each do |u| %>
    <table>
      <tr>
        <th>First name</th>
        <th>Last name</th>
        <th>Email</th>
      </tr>
      <tr>
        <td><%= u.first_name %></td>
        <td><%= u.last_name %></td>
        <td><%= u.email %></td>
      </tr>
    </table>
  <% end %>
</div>